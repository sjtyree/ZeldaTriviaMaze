<Window x:Name="LoadGamewindow" x:Class="ZeldaTriviaMaze.LoadGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadGameWindow" Height="300" Width="300" Loaded="LoadFiles">
    <Grid>
        <Button x:Name="LoadButton" Content="Load" HorizontalAlignment="Left" Height="31" Margin="24,228,0,0" VerticalAlignment="Top" Width="109" FontSize="16" Click="LoadButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Height="31" Margin="159,228,0,0" VerticalAlignment="Top" Width="109" FontSize="16" Click="CancelButton_Click"/>
        <ListBox x:Name="GameSavesListBox" HorizontalAlignment="Left" Height="161" Margin="24,38,0,0" VerticalAlignment="Top" Width="244" SelectionChanged="Selected_Click"/>

    </Grid>
</Window>
